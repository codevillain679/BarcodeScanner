<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.barcodescanner">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:screenOrientation="landscape"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Verdelen" />
        <activity android:name=".EndlessSummer" />
        <activity android:name=".Overig" />
        <activity android:name=".Struik" />
        <activity android:name=".Stam" />
        <activity android:name=".Klim" />
        <activity
            android:name=".VerdelenFS"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_verdelen_fs"
            android:theme="@style/FullscreenTheme" />
        <activity
            android:name=".MainActivityFS"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_main_fs"
            android:theme="@style/FullscreenTheme"></activity>
    </application>

</manifest>